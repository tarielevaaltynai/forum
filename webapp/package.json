{
  "name": "@forum_project/webapp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "jest",
    "types": "tsc --noEmit --project ./tsconfig.app.json && tsc --noEmit --project ./tsconfig.node.json ",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "preview": "vite preview",
    "prettify": "prettier --log-level warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@forum_project/backend": "workspace:*",
    "@forum_project/shared": "workspace:*",
    "@nanostores/react": "^0.7.1",
    "@tanstack/react-query": "^4.32.6",
    "@trpc/client": "^10.37.1",
    "@trpc/react-query": "^10.37.1",
    "@types/react-datepicker": "^7.0.0",
    "@trpc/server": "^10.37.1",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "font-awesome": "^4.7.0",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.0.1",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "nanostores": "^0.9.3",
    "lucide-react": "^0.479.0",
    "react": "^18.2.0",
    "@sentry/react": "^7.70.0",
    "react-datepicker": "^8.1.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.11.0",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^6.29.0",
    "reset-css": "^5.0.2",
    "sass": "^1.63.0",
    "superjson": "^1.13.3",
    "typeface-roboto": "^1.1.13",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.24.2"
